#- b
    nasm src/boot.asm -o out/boot.bin
    nasm src/kernel.asm -o out/kernel.bin
    dd if=/dev/zero of=out/osdev.img bs=1024 count=1440
    dd if=out/boot.bin of=out/osdev.img conv=notrunc
    dd if=out/kernel.bin of=out/osdev.img conv=notrunc seek=1
#/

#- r
    qemu-system-x86_64 -fda out/osdev.img -monitor stdio -serial file:out/log
#/
